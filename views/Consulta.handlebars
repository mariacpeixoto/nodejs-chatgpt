<div
  class="container text-center bg-body-tertiary"
  style="margin-top: 2%; margin-bottom: 2%;"
>
  <div class="col-1">
    &nbsp;
  </div>
  <div class="row">
    <nav class="navbar bg-primary" data-bs-theme="dark">
      <div class="container-fluid">
        <a class="navbar-brand" href="#" style="font-weight: 600;">SISTEMA WEB -
          CHAT GPT</a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarNavAltMarkup"
          aria-controls="navbarNavAltMarkup"
          aria-expanded="false"
          aria-label="Toggle navigation"
          style="background-color: #ffff;"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
          <div class="navbar-nav">
            <a class="nav-link" aria-current="page" href="/">Cadastrar</a>
            <a class="nav-link active" href="#">Consultar</a>
          </div>
        </div>
      </div>
    </nav>
  </div>
  <div class="row">
    <div class="card mb-3 col-12">
      <div class="card-body" style="margin: auto;">
        <div class="col-1">
          &nbsp;
        </div>
        <h5 class="card-title" style="font-weight: 700;">Consultar Produtos</h5>
        <div class="col-1">
          &nbsp;
        </div>
        <table class="table table-responsive" style="width: auto;">
          <thead class="table-active bg-primary">
            <tr>
              <th scope="col">Produto</th>
              <th scope="col">Marca</th>
              <th scope="col">Modelo</th>
              <th scope="col">Descrição</th>
              <th scope="col">Palavras-Chave</th>
            </tr>
          </thead>
          {{#each post}}
            {{#dataValues}}
              <tbody id="TableData">
                <tr>
                  <td scope="col">
                    {{produto}}
                  </td>
                  <td scope="col">
                    {{marca}}
                  </td>
                  <td scope="col">
                    {{modelo}}
                  </td>
                  <td scope="col">
                    {{descricao}}
                  </td>
                  <td scope="col">
                    {{palavras_chave}}
                  </td>
                </tr>
              </tbody>
            {{/dataValues}}
          {{/each}}
        </table>
        <div class="col-1">
          &nbsp;
        </div>
      </div>
    </div>
  </div>
</div>